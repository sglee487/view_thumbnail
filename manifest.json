{
    "name": "view thumbnail",
    "description": "Popup youtube thumbnail playing now.",
    "version": "0.2",
    "manifest_version": 3,
    "background": {
      "service_worker": "background.js"
    },
    "permissions": ["tabs"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "/images/3_16.png",
        "32": "/images/3_32.png",
        "48": "/images/3_48.png",
        "128": "/images/3_128.png"
      }
    },
    "icons": {
      "16": "/images/3_16.png",
      "32": "/images/3_32.png",
      "48": "/images/3_48.png",
      "128": "/images/3_128.png"
    }
  }
  